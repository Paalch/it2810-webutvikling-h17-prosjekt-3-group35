{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "enzyme": "^3.1.0",
    "enzyme-to-json": "^3.1.2",
    "jest": "^21.2.1",
    "moment": "^2.18.1",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "rc-time-picker": "^2.4.1",
    "react": "^15.6.2",
    "react-autosize-textarea": "^0.4.9",
    "react-bootstrap": "^0.31.3",
    "react-datepicker": "^0.55.0",
    "react-dom": "^15.6.2",
    "react-draggable": "^3.0.3",
    "react-hot-loader": "^3.1.1",
    "react-scripts": "1.0.13",
    "react-test-renderer": "^15.5.0",
    "react-timepicker": "^1.3.1",
    "sinon": "^4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "jest": "./node_modules/.bin/jest"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.1.0",
      "enzyme-adapter-react-15": "1.0.2",
    "enzyme-to-json": "^3.1.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-localstorage-mock": "^2.0.1",
      "react-test-renderer": "15.5.4"
  },
  "jest": {
    "setupFiles": [
    "jest-localstorage-mock"
    ] ,
    "rootDir": "src",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.svg$": "<rootDir>/__mocks__/svgMock.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**"
      ]
  }
}
